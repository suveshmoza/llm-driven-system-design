{
  "name": "price-tracking-backend",
  "version": "1.0.0",
  "description": "Price tracking service backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "dev:scraper": "tsx watch src/scraper-worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cockatiel": "^3.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pino": "^10.2.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^22.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/amqplib": "^0.10.8",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.0",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "globals": "^15.14.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.2"
  }
}
