{
  "name": "ad-click-aggregator-backend",
  "version": "1.0.0",
  "description": "Real-time ad click aggregation backend service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "pg": "^8.16.0",
    "pino": "^10.2.0",
    "prom-client": "^15.1.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.36"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/pg": "^8.15.4",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.18.2"
  }
}
