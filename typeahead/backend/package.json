{
  "name": "typeahead-backend",
  "version": "1.0.0",
  "description": "Typeahead/Autocomplete service with trie-based prefix matching",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "seed": "tsx src/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "opossum": "^9.0.0",
    "pg": "^8.11.3",
    "pino": "^10.2.0",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
