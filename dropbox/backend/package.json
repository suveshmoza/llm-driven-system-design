{
  "name": "dropbox-backend",
  "version": "1.0.0",
  "description": "Dropbox Cloud Storage Backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dropbox",
    "cloud-storage",
    "file-sync"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "bcrypt": "^5.1.1",
    "cockatiel": "^3.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.4.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
