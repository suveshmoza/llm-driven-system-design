{
  "name": "rate-limiter-backend",
  "version": "1.0.0",
  "description": "Distributed rate limiting service with multiple algorithms",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "ioredis": "^5.4.1",
    "opossum": "^9.0.0",
    "pg": "^8.13.0",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.11.10",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
