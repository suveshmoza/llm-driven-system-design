{
  "name": "strava-backend",
  "version": "1.0.0",
  "description": "Fitness tracking platform backend",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server1": "PORT=3001 tsx watch src/index.ts",
    "dev:server2": "PORT=3002 tsx watch src/index.ts",
    "dev:server3": "PORT=3003 tsx watch src/index.ts",
    "start": "node --import tsx src/index.ts",
    "db:rollback": "tsx src/db/migrate.ts rollback",
    "db:status": "tsx src/db/migrate.ts status",
    "db:create-migration": "tsx src/db/migrate.ts create",
    "migrate": "tsx src/migrate.ts",
    "seed": "tsx src/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fast-xml-parser": "^4.3.2",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pino": "^10.2.0",
    "pino-pretty": "^11.0.0",
    "polyline": "^0.2.0",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
